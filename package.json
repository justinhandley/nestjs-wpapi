{
  "contributors": [
    {
      "name": "Dexter Hardy",
      "email": "dexter.hardy@ntegral.com",
      "url": "http://www.ntegral.com"
    }
  ],
  "name": "@ntegral/nestjs-wpapi",
  "version": "1.0.1",
  "description": "Provides an injectable wordpress client to provide features to the Wordpress API for nestjs projects",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "nestjs",
    "wordpress",
    "node"
  ],
  "author": "Dexter Hardy",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/common": "^6.10.13",
    "@nestjs/core": "^6.10.13",
    "@nestjs/testing": "^6.10.13",
    "@types/jest": "^24.0.25",
    "@types/wpapi": "^1.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "wpapi": "^1.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
